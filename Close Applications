private void close_button(object sender, EventArgs e)
        {
            DialogResult result = MessageBox.Sho∆∞("Do you want to close <Name of aplication> ?","<Name of aplication>", MessageBoxButtons.YesNo,MessageBoxIcon.Question);
            if (result == DialogResult.Yes)
            {
                Application.Exit();
            }
            else return;
        }
